{
  "name": "chance",
  "version": "2.0.0",
  "description": "Chance - Utility library to generate anything random",
  "homepage": "http://chancejs.com",
  "author": "Victor Quinn <mail@victorquinn.com>",
  "private": true,
  "workspaces": [
    "packages/mersenne-twister",
    "packages/blue-imp-md5",
    "packages/generator",
    "packages/bool",
    "packages/integer",
    "packages/floating",
    "packages/natural",
    "packages/falsy",
    "packages/character",
    "packages/letter",
    "packages/hex",
    "packages/pick",
    "packages/string",
    "packages/chance"
  ],
  "engines": {
    "yarn": ">=1",
    "node": ">=14"
  },
  "bugs": {
    "url": "https://github.com/chancejs/chancejs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chancejs/chancejs.git"
  },
  "license": "MIT",
  "devDependencies": {
    "docpress": "0.7.1",
    "git-update-ghpages": "1.3.0",
    "nyc": "^15.1.0",
    "plop": "^3.1.0"
  },
  "scripts": {
    "clean": "yarn workspaces run clean",
    "lint": "yarn workspaces run lint",
    "tsc": "yarn workspaces run tsc",
    "build": "yarn clean && yarn tsc",
    "test": "yarn workspaces run test",
    "coverage": "./scripts/aggregate-coverage.sh",
    "docs": "docpress build",
    "docs:publish": "git-update-ghpages -e",
    "package:create": "plop --plopfile ./scripts/create-package/plopfile.js",
    "generator:create": "plop --plopfile ./scripts/create-generator/plopfile.js",
    "method:add": "plop --plopfile ./scripts/add-method/plopfile.js"
  },
  "keywords": [
    "chance",
    "random",
    "generator",
    "test",
    "mersenne",
    "name",
    "address",
    "dice"
  ],
  "github": "https://github.com/chancejs/chancejs",
  "maintainers": [
    {
      "name": "Victor Quinn",
      "email": "mail@victorquinn.com",
      "web": "https://www.victorquinn.com"
    }
  ],
  "docpress": {
    "scripts": [
      "chance.js",
      "analytics.js",
      "https://platform.twitter.com/widgets.js"
    ],
    "github": "chancejs/chancejs",
    "css": [
      "docs/chance.css"
    ]
  }
}
